Box :: struct {
    a: s64,
    b: s64,
    c: s64,
    d: s64,
    e: s64,
}

ptr_next :: (x: *s64) -> *s64 {
    ret x + 1;
}

ptr_next_arb :: (x: *s64, y: s64) -> *s64 {
    ret x + y;
}

main :: (argc: s64, argv: **char) -> s64 {
    let box: Box;
    box.a = 67;
    box.b = 42;
    box.c = 15;
    box.d = 121;
    box.e = 75;

    ret *ptr_next_arb(&box.a, 0);
}
