memcpy :: (dst: *void, src: *void, n: mut u64) -> void {
    let dstp: *char = dst;
    let srcp: *char = src;
    let i: u64 = 0;

    while i < n {
        dstp[i] = srcp[i];
        ++i;
    }
}

main :: (argc: s64, argv: **char) -> s64 {
    let a: s64 = 42;
    let b: s64 = 141;

    memcpy(&a, &b, sizeof(s64));

    ret a;
}
